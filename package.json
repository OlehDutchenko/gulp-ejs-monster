{
  "name": "gulp-ejs-monster",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "gulpEjsMonster": {
    "divider": "\n-----------",
    "newline": "NEWLINE_FILENAME::"
  },
  "scripts": {
    "code-style": "happiness \"./utils/**/*.js\" \"./methods/**/*.js\" \"./*.js\" --verbose | snazzy",
    "code-style-fix": "happiness \"./utils/**/*.js\" \"./methods/**/*.js\" \"./*.js\" --verbose --fix | snazzy",
    "test": "npm run code-style"
  },
  "happiness": {
    "ignore": "./examples/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dutchenkoOleg/gulp-ejs-monster.git"
  },
  "engines": {
    "node": ">=6.3.1"
  },
  "keywords": [
    "ejs",
    "gulp",
    "gulp-plugin",
    "template-engine"
  ],
  "author": "Oleg Dutchenko <dutchenko.o.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dutchenkoOleg/gulp-ejs-monster/issues"
  },
  "homepage": "https://github.com/dutchenkoOleg/gulp-ejs-monster#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "ejs": "^2.5.7",
    "ejs-lint": "^0.3.0",
    "esprima": "^4.0.0",
    "gulp-not-supported-file": "^1.2.0",
    "gulp-util": "^3.0.8",
    "js-beautify": "^1.6.14",
    "lodash": "^4.5.0",
    "markdown": "^0.5.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-mocha": "^4.3.1",
    "happiness": "^7.1.2",
    "jsdoc-simple-theme": "^1.1.0",
    "should": "^11.2.1",
    "snazzy": "^7.0.0",
    "yargs": "^8.0.2"
  }
}
